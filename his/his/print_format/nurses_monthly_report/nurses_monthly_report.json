 "html": "<style>\r\nhtml {\r\n    font-size: 100%;\r\n    box-sizing: border-box\r\n}\r\n\r\n*,\r\n*::before,\r\n*::after {\r\n    box-sizing: inherit\r\n}\r\n\r\nbody {\r\n    padding: 0;\r\n    margin: 0;\r\n    font-family: \"Public Sans\", sans-serif;\r\n    font-size: .875rem;\r\n    font-weight: 300;\r\n    color: #000\r\n}\r\n\r\nh1,\r\nh2,\r\nh3 {\r\n    font-weight: 300;\r\n    color: #000;\r\n    margin-top: 0\r\n}\r\n\r\np {\r\n    margin-top: 0\r\n}\r\n\r\na,\r\na:visited,\r\na:hover {\r\n    text-decoration: none\r\n}\r\n\r\n.marginX {\r\n    margin-inline: 50px\r\n}\r\n\r\n.header {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    -webkit-margin-before: 19px;\r\n    margin-block-start: 19px;\r\n    justify-content: center;\r\n    padding-bottom: 8px;\r\n    /*border-bottom: 1px solid #000*/\r\n}\r\n\r\n.header__top {\r\n    display: flex;\r\n    align-items: center;\r\n    gap: 9px\r\n}\r\n\r\n.header__logo {\r\n    width: 70px;\r\n    height: 70px;\r\n    -o-object-fit: contain;\r\n    object-fit: contain;\r\n    padding-bottom: 9px\r\n}\r\n\r\n.header__title {\r\n    font-weight: bold;\r\n    text-transform: uppercase\r\n}\r\n\r\n.header__midd {\r\n    display: flex;\r\n    align-items: center;\r\n    margin-left: 39px\r\n}\r\n\r\n.header__phone {\r\n    font-size: 1.125rem\r\n}\r\n\r\n.header__bottom {\r\n    display: flex;\r\n    flex-direction: column;\r\n    gap: 3px;\r\n    margin-left: 59px\r\n}\r\n\r\n.header__gmail {\r\n    font-size: 1.125rem\r\n}\r\n\r\n.header__country {\r\n    font-size: 1.125rem;\r\n    text-align: center\r\n}\r\n\r\n.banner {\r\n    display: flex;\r\n    flex-direction: column;\r\n    -webkit-margin-before: 10px;\r\n    margin-block-start: 10px;\r\n    gap: 8px\r\n}\r\n\r\n.banner__title {\r\n    text-align: center;\r\n    font-weight: bold;\r\n    font-size: 1.0rem;\r\n    text-transform: uppercase\r\n}\r\n\r\n.banner__top {\r\n    display: flex;\r\n    align-items: center;\r\n  